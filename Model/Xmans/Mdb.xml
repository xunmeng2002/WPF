<Root>
	<Tables len="32">
		<Table name="Order">
			<Item name="AccountID" type="string"/>
			<Item name="ExchangeID" type="string"/>
			<Item name="InstrumentID" type="string"/>
			<Item name="OrderSysID" len ="64" type="string"/>
			<Item name="Direction" type="Direction"/>
			<Item name="OrderPriceType" type="OrderPriceType"/>
			<Item name="Price" type="double"/>
			<Item name="Volume" type="int"/>
			<Item name="VolumeTraded" type="int"/>
			<Item name="OrderStatus" type="OrderStatus"/>
			<Item name="RequestID" type="long"/>
			<Item name="SessionID" type="long"/>
			<Item name="InsertDate" type="string"/>
			<Item name="InsertTime" type="string"/>
			<Item name="ErrorID" type="int"/>
			<Item name="ErrorMsg" type="string" len="255"/>
		</Table>
		<Table name="Trade">
			<Item name="AccountID" type="string"/>
			<Item name="ExchangeID" type="string"/>
			<Item name="InstrumentID" type="string"/>
			<Item name="TradeID" type="string"/>
			<Item name="Direction" type="Direction"/>
			<Item name="Price" type="double"/>
			<Item name="Volume" type="int"/>
			<Item name="OrderLocalID" type="string"/>
			<Item name="OrderSysID" len ="64" type="string"/>
			<Item name="TradeTime" type="string"/>
			<Item name="TradeDate" type="string"/>
		</Table>
		<Table name="Position">
			<item name="AccountID" type="string"/>
			<item name="ExchangeID" type="string"/>
			<item name="InstrumentID" type="string"/>
			<item name="Direction" type="Direction"/>
			<item name="OpenPrice" type="double"/>
			<item name="LastPrice" type="double"/>
			<item name="Volume" type="int"/>
			<item name="Profit" type="double"/>
		</Table>
		<Table name="BasketStrategy">
			<item name="AccountID" type="string"/>
			<Item name="OrderSysID" type="string"/>
			<Item name="ExecuteType" type="int"/>
			<Item name="StrategyType" type="int"/>
			<Item name="AscendSpeed" type="int"/>
			<Item name="TradeRate" type="int"/>
			<Item name="CancelRate" type="int"/>
			<Item name="RemainVolume" type="int"/>
			<item name="Formula" type="string"/>
			<item name="GapTime" type="long"/>
			<item name="BeginTime" type="int"/>
			<item name="EndTime" type="int"/>
			<item name="BasketFile" type="string"/>
			<Item name="RequestID" type="long"/>
			<Item name="SessionID" type="long"/>
		</Table>
	</Tables>
</Root>