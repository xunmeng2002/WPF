<root>
	<enums>
		<enum name="OrderStatus">
			<item name="Inserting" value="0" desc="正在申报"/>
			<item name="Inserted" value="1" desc="已报"/>
			<item name="PartTraded" value="2" desc="部成"/>
			<item name="AllTraded" value="3" desc="已成"/>
			<item name="Canceled" value="4" desc="已撤"/>
			<item name="PartTradedCanceled" value="5" desc="部成部撤"/>
			<item name="Error" value="e" desc="废单"/>
			<item name="NotTouched" value="p" desc="未触发"/>
			<item name="Touched" value="t" desc="已触发"/>
			<item name="Unknown" value="x" desc="未知"/>
		</enum>
		<enum name="Direction">
			<item name="Buy" value="0" desc="买"/>
			<item name="Sell" value="1" desc="卖"/>
		</enum>
		<enum name="HedgeFlag">
			<item name="Speculation" value="1" desc="投机"/>
			<item name="Arbitrage" value="2" desc="套利"/>
			<item name="Hedge" value="3" desc="保值"/>
		</enum>
		<enum name="OrderPriceType">
			<item name="Anyprice" value="1" desc="市价"/>
			<item name="LimitPrice" value="2" desc="限价"/>
			<item name="BestPrice" value="3" desc="最优价"/>
			<item name="LastPrice" value="4" desc="最新价"/>
		</enum>
		<enum name="OffsetFlag">
			<item name="Open" value="0" desc="开仓"/>
			<item name="Close" value="1" desc="平仓"/>
			<item name="CloseToday" value="2" desc="平今"/>
		</enum>
		<enum name="ContingentCondition">
			<item name="Immediately" value="1" desc="立即"/>
			<item name="Touch" value="2" desc="止损"/>
			<item name="TouchProfit" value="3" desc="止赢"/>
			<item name="ParkedOrder" value="4" desc="预埋单"/>
		</enum>
		<enum name="TimeCondition">
			<item name="IOC" value="1" desc="立即完成，否则撤销"/>
			<item name="GFS" value="2" desc="本节有效"/>
			<item name="GFD" value="3" desc="当日有效"/>
