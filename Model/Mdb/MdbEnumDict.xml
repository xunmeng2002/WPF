			<item name="GTC" value="5" desc="撤销前有效"/>
			<item name="GFA" value="6" desc="集合竞价有效"/>
		</enum>
		<enum name="VolumeCondition">
			<item name="AV" value="1" desc="任何数量"/>
			<item name="MV" value="2" desc="最小数量"/>
			<item name="CV" value="3" desc="全部数量"/>
		</enum>
		<enum name="ForceCloseReason">
			<item name="NotForceClose" value="0" desc="非强平"/>
			<item name="LackDeposit" value="1" desc="资金不足"/>
			<item name="ClientOverPositionLimit" value="2" desc="客户超仓"/>
			<item name="MemberOverPositionLimit" value="3" desc="会员超仓"/>
			<item name="NotMultiple" value="4" desc="持仓非整数倍"/>
