			<item name="Other" value="6" desc="其它"/>
			<item name="PersonDeliv" value="7" desc="自然人临近交割"/>
		</enum>
		<enum name="TradeType">
			<item name="Common" value="0" desc="普通成交"/>
			<item name="OptionsExecution" value="1" desc="期权执行"/>
			<item name="OTC" value="2" desc="OTC成交"/>
			<item name="EFPDerived" value="3" desc="期转现衍生成交"/>
			<item name="CombinationDerived" value="4" desc="组合衍生成交"/>
		</enum>
		<enum name="MarginPriceType">
			<item name="PreSettlementPrice" value="1" desc="昨结算价"/>
			<item name="SettlementPrice" value="2" desc="最新价"/>
			<item name="AveragePrice" value="3" desc="成交均价"/>
			<item name="OpenPrice" value="4" desc="开仓价"/>
		</enum>
		<enum name="IsLocalOrder">
			<item name="Others" value="0" desc="外部"/>
			<item name="Local" value="1" desc="本地"/>
		</enum>
